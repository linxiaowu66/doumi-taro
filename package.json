{
  "name": "doumi-taro",
  "version": "1.0.0",
  "description": "RN/micro-program/h5 version for doumi blog(https://blog.5udou.cn)",
  "main": "index.js",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "lint": "eslint ./src --cache --fix --ignore-pattern .gitignore",
    "upload": "npm run build:h5 && npm run buildload && node ./bin/upload.js",
    "upload:img": "node ./bin/uploadImg.js",
    "deploy": "node ./bin/commander.js sp"
  },
